<?xml version="1.0" encoding="UTF-8"?>
<schemalist>

    <schema id="com.github.arshubham.gitignore" path="/com/github/arshubham/gitignore/">
		<key name="window-maximized" type="b">
			<default>false</default>
			<summary>Whether the window was maximized on last run</summary>
			<description>Whether the window was maximized on last run</description>
		</key>
		<key name="window-position" type="(ii)">
			<default>(-1, -1)</default>
			<summary>Window position</summary>
			<description>Most recent window position (x, y)</description>
		</key>
			<key name="window-size" type="(ii)">
			<default>(700, 500)</default>
			<summary>Most recent window size</summary>
			<description>Most recent window size (width, height)</description>
		</key>
		<key name="prefer-dark" type="b">
			<default>false</default>
			<summary>Prefer dark theme</summary>
			<description>Prefer dark theme</description>
        </key>
        <key name="selected-langs" type="as">
			<default>[]</default>
			<summary>Selected Languages</summary>
			<description>Selected Languages at a moment to be shown in Tags</description>
        </key>
    </schema>
</schemalist>
